<div class="d-flex justify-content-center">
  <div class="row">
    <%@listings.each do |listing|%>
      <div class="col-4">
        <div class="card mb-4">
          <img class="card-img-top" src="https://images.unsplash.com/photo-1531722569936-825d3dd91b15?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=2070&q=80" alt="Card image cap">
          <div class="card-body">
            <h5 class="card-title"><%=listing.title%></h5>
            <p class="card-text"><%=listing.description%></p>
            <a href="#" class="btn btn-primary">More Details</a>
          </div>
        </div>
      </div>
    <%end%>
  </div>
</div>
