<div class="container mt-3">
  <div class="mb-3">
    <h1>Dashboard</h1>
  </div>

  <ul class="nav nav-tabs" id="myTab" role="tablist">
    <li class="nav-item" role="presentation">
      <button class="nav-link active" id="bookings-tab" data-bs-toggle="tab" data-bs-target="#home" type="button" role="tab" aria-controls="home" aria-selected="true">Bookings</button>
    </li>
    <li class="nav-item" role="presentation">
      <button class="nav-link" id="listings-tab" data-bs-toggle="tab" data-bs-target="#profile" type="button" role="tab" aria-controls="profile" aria-selected="false">Listings</button>
    </li>
  </ul>
  <div class="tab-content mt-3" id="myTabContent">
    <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="bookings-tab">
      <%=render "booking_card"%>
    </div>
    <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="listings-tab">
      <%=render "listing_card"%>
    </div>
  </div>


</div>
